#include <stdio.h>

int main()
{
  float fahrenheit;
  float celsius;
  
    printf("enter temperature in fahrenheit");
    scanf("%d",&fahrenheit ); 
    
    celsius=(fahrenheit-32)*5/9;
    
    printf("temprature in celsius is %f",celsius);
    
    return 0;
    
}
