#include <stdio.h>

int main() {
    int sum=0;
    int n;
    printf("Enter number to be printed consecutively:");
    scanf("%d",&n);
      
    for(int i=1;i<=n;i++)
   { 
     sum=sum+i;
     printf("sum of first %d is:%d\n",i,sum);
   }
    return 0;
}
